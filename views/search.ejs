<div class="row">
    <div class="left-part col-xl-8   col-md-6 col-sm-12 ">
        <div class="row">
          <% persons.forEach( function(model) { %>
          <div class=" card col-xl-6 col-md-12 col-sm-12">
          <img class="card-img-top img-fluid" src="<%=model.imageUrl%>" alt="Card image cap">
          <div class="card-body">
            <h4 class="card-title"><a href="www.baidu.com" class="card-title-link"><%=model.city%></a></h4>
            <p class="card-text"><%=model.class%> class, <%=model.price%></p>
            <p class="card-text">Valid till:<%=model.dataValidTill%>.Quota left:<%=model.overallQuota%></p>
            <p class="card-text text-secondary h6"><%=model.travelDates%></p>
          </div>
        </div>
         <% }); %>
         <div class="col-xl-12">
           <nav aria-label="Page navigation example">
  <ul class="pagination">
    <li class="page-item">
      <a class="page-link" href="/person/search" aria-label="Previous">
        <span aria-hidden="true">&laquo;</span>
        <span class="sr-only">Previous</span>
      </a>
    </li>
    <li class="page-item"><a class="page-link" href="/person/search">1</a></li>
    <li class="page-item"><a class="page-link" href="/person/search">2</a></li>
    <li class="page-item"><a class="page-link" href="/person/search">3</a></li>
    <li class="page-item">
      <a class="page-link" href="/person/search" aria-label="Next">
        <span aria-hidden="true">&raquo;</span>
        <span class="sr-only">Next</span>
      </a>
    </li>
  </ul>
</nav>
         </div>
        </div>
  </div>
  <from class="right-part col-xl-4 col-md-6 col-sm-12 ">
    <div class="h4 ">Search</div>
    <div class="form-group">
   <label for="exampleFormControlSelect1">Region</label>
   <select class="form-control" id="exampleFormControlSelect1">
              <option value="East Asia">East Asia</option>
              <option value="South-east Asia">South-east Asia</option>
              <option value="Americas">Americas</option>
   </select>
 </div>
 <div class="form-group">
    <label for="exampleFormControlInput1">Price Range</label>
    <input type="text" class="form-control" id="price" >
  </div>
   <div class="form-group">
    <label for="exampleFormControlInput1">Date Range</label>
    <input type="text" class="form-control" id="dates" >
  </div>
  <a href="#" class="btn btn-primary btn-lg active" role="button" aria-pressed="true">Submit</a>

  </from>



  </div>
      <script src="/js/jquery-3.2.1.min.js"></script>
          <script src="/js/moment.min.js"></script>
          <script src="/js/bootstrap-datepicker.min.js"></script>
              <script src="/js/daterangepicker.js"></script>
    <script src="/js/ion-rangeSlider/ion.rangeSlider.min.js"></script>
    <script src="/js/moment.min.js"></script>
    <script>$("#price").ionRangeSlider({
      type: "double",
      grid: true,
      min: 1000,
      max: 10000,
      from: 1000,
      to: 10000,
      prefix: "$"
    });
    $("#dates").daterangepicker({
            "startDate": "01/01/2017",
            "endDate": "12/30/2017"
        });
      </script>
