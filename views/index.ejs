
  <div class="row">
    <% persons.forEach( function(model) { %>
    <div class="card col-xl-4 col-md-6 col-sm-12" onclick="detail('<%= model.id%>')">
    <img class="card-img-top img-fluid" src="<%=model.imageUrl%>" alt="Card image cap">
      <div class="card-body">
        <h4 class="card-title"><class="card-title-link"><%=model.city%></a></h4>
        <p class="card-text"><%=model.class%> class, <%=model.price%></p>
        <p class="card-text">Valid till:<%=model.dataValidTill%>.Quota left:<%=model.overallQuota%></p>
        <p class="card-text text-secondary h6"><%=model.travelDates%></p>
      </div>
   </div>
   <% }); %>
     <div class="col-xl-12">
           <nav aria-label="Page navigation example">
           <ul class="pagination">
           <li class="page-item">
              <a class="page-link" href="/person/index" aria-label="Previous">
                 <span aria-hidden="true">&laquo;</span>
                 <span class="sr-only">Previous</span>
             </a>
          </li>
          <li class="page-item"><a class="page-link" href="/person/index">1</a></li>
          <li class="page-item">
             <a class="page-link" href="/person/index" aria-label="Next">
                <span aria-hidden="true">&raquo;</span>
                <span class="sr-only">Next</span>
             </a>
          </li>
          </ul>
          </nav>
    </div>
  </div>

  <script>
      function detail(i) {
          window.location.href='/person/view/'+i;
      }
  </script>
